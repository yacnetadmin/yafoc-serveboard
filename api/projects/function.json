{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [ "get", "post", "options" ],
      "route": "projects"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "cors": {
    "allowedOrigins": [
      "https://yacnetadmin.github.io"
    ],
    "allowedMethods": "GET,POST,OPTIONS",
    "allowedHeaders": "Content-Type,Authorization",
    "allowCredentials": true
  }
}
